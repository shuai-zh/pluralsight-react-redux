{
  "name": "pluralsight-redux-starter",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "author": "Shuai Zhang",
  "license": "MIT",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "open:dist": "cross-env NODE_ENV=production babel-node buildScripts/distServer.js",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test nyc mocha \"./src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "start:dist": "npm-run-all build open:dist"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "chalk": "^2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "compression": "^1.7.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^9.0.14",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.3",
    "open": "^0.0.5",
    "react-test-renderer": "^15.6.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shuai-zh/pluralsight-react-redux"
  },
  "nyc": {
    "require": [
      "./buildScripts/testSetup.js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "all": true
  }
}
